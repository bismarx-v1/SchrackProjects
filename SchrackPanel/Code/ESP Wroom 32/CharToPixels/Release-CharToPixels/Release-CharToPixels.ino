void TimeToPixels(uint8_t* TTP_Time, uint8_t* TTP_Frame, uint8_t TTP_Highlight) {
	for(uint8_t Char = 0; Char <= 6-1; Char++) {
		Serial.printf("%i", TTP_Time[Char]);
		if(Char==1||Char==3) {
			Serial.printf(":");
		}
		
		for(uint8_t y = 0; y <= 7-1; y++) {
			for(uint8_t x = 0; x <= 5-1; x++) {
				TTP_Frame[x+y*48+TimeCharOffset[Char]] = Charset[x+y*5+35*(TTP_Time[Char]+1)];
			}
			
			if(Char==1||Char==3) {
				for(uint8_t x = 0; x <= 3-1; x++) {
					TTP_Frame[x+5+y*48+TimeCharOffset[Char]] = Charset[x+y*5+35*63+1];
				}
			}
		}
	}
	Serial.printf("\n");
	
	for(uint16_t i = 0; i <= 336-1; i++) {
		if(i%48>=42) {
			TTP_Frame[i] = TTP_Highlight;
		}
	}
	
	return;
}
// </TimeToPixels>

// <StringToPixels>
void StringToPixels(uint8_t* STP_Text, uint8_t* STP_Frame, uint8_t STP_Highlight, uint16_t STP_Offset) {
	uint8_t VirtFrame[(sizeof(STP_Text)*6)*7];
	
	for(uint8_t CharPos = 0; CharPos <= sizeof(STP_Text)-1; CharPos++) {
		for(uint8_t y = 0; y <= 7-1; y++) {
			for(uint8_t x = 0; x <= sizeof(STP_Text)-1; x++) {
				VirtFrame[y*sizeof(STP_Text)+x+CharPos*6+STP_Offset] = Charset[x+y*5+35*CharToCharIndex(1)];
			}
		}
	}
	
	//STP_Frame[x+y*48] = VirtFrame[y*48+x+STP_Offset];
	for(uint8_t y = 0; y <= 7-1; y++) {
		for(uint8_t x = 0; x <= 48-1; x++) {	// 48 or 42
			STP_Frame[x+y*48] = VirtFrame[y*48+x+STP_Offset];
		}
	}
	
	Serial.printf("THIS %i\n", sizeof(STP_Text));
	for(uint8_t y = 0; y <= 7-1; y++) {
		for(uint8_t x = 0; x <= sizeof(STP_Text)*6-1; x++) {
			Serial.printf("%i ", VirtFrame[y*sizeof(STP_Text)+x]);
		}
		
		Serial.printf("\n");
	}
	
	Serial.printf("THAT\n");
}
// </StringToPixels>

void setup() {
	Serial.begin(115200);
	
	// <set frame to 2>
	ResetFrame(Frame);
	// </set frame to 2>
	
	if(PrintModeTime) {	// setup for printing time
		// <declare time>
		//uint8_t* Time;
		Time = new uint8_t[6];
		// </declare time>
		
		// <set time>
		Time[0] = 0;
		Time[1] = 1;
		Time[2] = 5;
		Time[3] = 9;
		Time[4] = 5;
		Time[5] = 9;
		// </set time>
		
		TimeToPixels(Time, Frame, Time[5]%2);
	} else {	// setup for printing text
		uint8_t Text[16] = {'H', 'e', 'w', 'w', 'o', ' ', 'W', 'o', 'w', 'd', '&', 'T', 'e', 's', 't', '0'};
		StringToPixels(Text, Frame, 1, 0);
	}
	
	PrintToConsole(Frame);
	
	
	while(1) {
		if(PrintModeTime) {	// printing time
			Time = DecrTime(Time);	// decrement time
			
			// <print time>
			TimeToPixels(Time, Frame, Time[5]%2);
			// </print time>
		} else {	// printing text
			break;
		}
		
		// <print>
		PrintToConsole(Frame);
		// </print>
		
		if(PrintModeTime) {	// if printing time
			// check if at end
			if(CheckTimeEnd(Time)) {
				break;
			}
		} else {	// if printing text
			break;	// program doesn't reach this tho
		}
	}
	
}






