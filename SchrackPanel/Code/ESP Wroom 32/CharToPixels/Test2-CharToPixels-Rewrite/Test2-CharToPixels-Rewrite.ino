/*=============================================================*\
* 7 char string turned into a frame using the format for time	*
\*-------------------------------------------------------------*/
int8_t** TimeToFrame(char* TTF_String) {
	int8_t** TTF_Frame;
	TTF_Frame = new int8_t*[7];
	for(int8_t i = 0; i<= 7; i++) {
		TTF_Frame[i] = new int8_t[48];
	}
	
	int8_t TTF_TimeOffset[7] = {0, 6, 14, 20, 28, 34, 40};
	
	for(int8_t y = 0; y <= 7-1; y++) {	// row
		for(int8_t i = 0; i <= 7-1; i++) {	// character
			for(int8_t x = 0; x <= 5-1; x++) {	// column of character
				TTF_Frame[y][x+TTF_TimeOffset[i]] = Charset[x+y*5+CharToCharIndex(TTF_String[i])*35];
			}
			
			if((i <= 1)||(i <= 3)) {	// in time for colon
				for(int8_t x = 0; x <= 3-1; x++) {	// column of colon
					TTF_Frame[y][x+TTF_TimeOffset[i]+5] = Charset[x+1+y*5+63*35];	// 63 is ':'
				}
			}
		}
	}
	
	return(TTF_Frame);
}
