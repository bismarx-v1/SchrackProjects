// converts a character to it's index
int CharToCharIndex(char FuncVar_Char) {
	if(FuncVar_Char==32) {	// if ' '
		return(0);
	} else if((FuncVar_Char>=48)&&(FuncVar_Char<=57)) {	// if [Num]
		return(FuncVar_Char-47);
	} else if((FuncVar_Char>=65)&&(FuncVar_Char<=90)) {	// if [UpperCase]
		return(FuncVar_Char-54);
	} else if((FuncVar_Char>=97)&&(FuncVar_Char<=122)) {	// if [LowerCase]
		return(FuncVar_Char-60);
	} else if(FuncVar_Char==58) {	// if ':'
		return(63);
	} else if(FuncVar_Char==45) {	// if '-'
		return(64);
	} else if(FuncVar_Char==43) {	// if '+'
		return(65);
	} else if(FuncVar_Char==42) {	// if '*'
		return(66);
	} else if(FuncVar_Char==95) {	// if '_'
		return(67);
	} else if(FuncVar_Char==46) {	// if '.'
		return(68);
	} else if(FuncVar_Char==44) {	// if ','
		return(69);
	} else if(FuncVar_Char==63) {	// if '?'
		return(70);
	} else if(FuncVar_Char==33) {	// if '!'
		return(71);
	} else if(FuncVar_Char==59) {	// if ';'
		return(72);
	} else if(FuncVar_Char==219) {	// if '█'
		return(73);
	} else if(FuncVar_Char==47) {	// if '/'
		return(74);
	} else if(FuncVar_Char==92) {	// if '\\'
		return(75);
	} else if(FuncVar_Char==124) {	// if '|'
		return(76);
	} else if(FuncVar_Char==40) {	// if '('
		return(77);
	} else if(FuncVar_Char==41) {	// if ')'
		return(78);
	} else if(FuncVar_Char==91) {	// if '['
		return(79);
	} else if(FuncVar_Char==93) {	// if ']'
		return(80);
	} else if(FuncVar_Char==123) {	// if '{'
		return(81);
	} else if(FuncVar_Char==125) {	// if '}'
		return(82);
	} else if(FuncVar_Char==60) {	// if '<'
		return(83);
	} else if(FuncVar_Char==62) {	// if '>'
		return(84);
	} else if(FuncVar_Char==61) {	// if '='
