// the god stuff
// converts a string to a 2D int array later usualy called "Frame"
// Frame is the var sent to the "LED Display Driver"
char** CreateFrame(char* CF_String, int CF_Mode) {	// the define of "CreateFrame()"; char* CF_String - the string to display; int CF_Mode - the display mode
	char** CF_Frame;	// <CreateFrameVar>	// define an char array - char = 1 byte while int = 2 bytes
	CF_Frame = new char*[7];	// add 7 rows of arrays
	for(int FrameYI = 0; FrameYI<= 6; FrameYI++) {	// for each row
		CF_Frame[FrameYI] = new unsigned char[48];	// add 48 columns of chars
		for(int FrameXI = 0; FrameXI <= 47; FrameXI++) {	// set every pos  to hold 2
			CF_Frame[FrameYI][FrameXI] = 2;
		}
	}				// </CreateFrameVar>

	if((CF_Mode==0)&&((len(CF_String)<7)||(len(CF_String)>7))) { // if mode = 0(time) return if not 7 chars
		return(CF_Frame);
	} else if((CF_Mode==1)&&((len(CF_String)<8)||(len(CF_String)>8))) { // if mode = 1(text) return if not 8 chars
		return(CF_Frame);
	}
	
}
