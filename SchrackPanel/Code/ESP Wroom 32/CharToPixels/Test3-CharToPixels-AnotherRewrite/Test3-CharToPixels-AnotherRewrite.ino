	TimeToCharList[7] = 0;
	TimeToCharList[8] = TTP_Time[4]+48;
	TimeToCharList[9] = 0;
	TimeToCharList[10] = TTP_Time[5]+48;
	
	for(uint8_t CharI = 0; CharI <= 10; CharI++) {	// component position (component = a char, short space, short colon)
		for(uint8_t CharY = 0; CharY <= 6; CharY++) {	// component's Y
			for(uint8_t CharX = 0; CharX <= CharXLoopOffset[CharI]; CharX++) {	// component's X
				if(CharXLoopOffset[CharI]==4) {			// print char
					TTP_RetFrame[CharX+CharY*48+CharXFrameOffset[CharI]] = Charset[CharY*5+CharX+CharToCharIndex(TimeToCharList[CharI]+1)*35];
				} 
				else if(CharXLoopOffset[CharI]==0) {	// print short space
					TTP_RetFrame[CharX+CharY*48+CharXFrameOffset[CharI]] = 0;
				} 
				else if(CharXLoopOffset[CharI]==2) {	// print short colon
					TTP_RetFrame[CharX+CharY*48+CharXFrameOffset[CharI]] = Charset[CharY*5+CharX+1+63*35];
				} else {
					TTP_RetFrame[CharX+CharY*48+CharXFrameOffset[CharI]] = 2;
				}
			}
		}
	}
	
	return(TTP_RetFrame);
}
