	for(uint8_t i = 0; i <= 6-1; i++) {
		Serial.printf("%i", Time[i]);
		if((i == 1)||(i == 3)) {
			Serial.printf(":");
		}
	}
	
	Serial.printf("\n");
	// </print time>
	
	// <print Frame>
	for(uint8_t FrameY = 0; FrameY <= 7-1; FrameY++) {
		Serial.printf("%02i|", FrameY);
		for(uint8_t FrameX = 0; FrameX <= 48-1; FrameX++) {
			if(Frame[FrameY*7+FrameX] == 1) {
				Serial.printf("@@@");
			} else if(Frame[FrameY*7+FrameX] == 0) {
				Serial.printf("^^^");
			} else {
				Serial.printf("...");
			}
		}
		
		Serial.printf("\n");
	}
	
	Serial.printf("   ");
	for(uint8_t FrameX = 0; FrameX <= 48-1; FrameX++) {
		if(FrameX <= 41) {
			Serial.printf("%02i|", FrameX);
		} else {
			Serial.printf("%02i^", FrameX);
		}
		
	}
	// </print Frame>
	
	while(1) {
		break;	// DEBUG
		Time = DecrTime(Time);	// decrement time
		
		// <print time>
		for(uint8_t i = 0; i <= 6-1; i++) {
			Serial.printf("%i", Time[i]);
			if((i == 1)||(i == 3)) {
				Serial.printf(":");
			}
		}
		
		Serial.printf("\n");
		// </print time>
		
		// check if at end
		if((Time[0]==0)&&(Time[1]==0)&&(Time[2]==0)&&(Time[3]==0)&&(Time[4]==0)&&(Time[5]==0)) {
			break;
		}
	}
}






