	Serial.begin(115200);
	
	// <set frame to 2>
	for(uint8_t y = 0; y <= 7-1; y++) {
		for(uint8_t x = 0; x <= 48-1; x++) {
			Frame[y*7+x] = (y+x)%2;
		}
	}
	// </set frame to 2>
	
	// <declare time>
	uint8_t* Time;
	Time = new uint8_t[6];
	// </declare time>
	
	// <set time>
	Time[0] = 0;
	Time[1] = 1;
	Time[2] = 5;
	Time[3] = 9;
	Time[4] = 5;
	Time[5] = 9;
	// </set time>
	
	Serial.printf("THIS");
	
	uint8_t* TempFrame;
	TempFrame = TimeToPixels(Time);
	
	Serial.printf("THAT");
	
	/*for(uint8_t i = 0; i <= 336-1; i++) {
		Frame[i] = TempFrame[i];
