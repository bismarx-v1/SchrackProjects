					TTP_Frame[x+5+y*48+CharOffset[Char]] = Charset[x+y*5+35*63+1];
				}
			}
		}
	}
	Serial.printf("\n");
	
	for(uint16_t i = 0; i <= 336-1; i++) {
		if(i%48>=42) {
			TTP_Frame[i] = TTP_Highlight;
		}
	}
	
	return;
}
// </TimeToPixels>

void setup() {
	Serial.begin(115200);
	
	// <set frame to 2>
	for(uint8_t y = 0; y <= 7-1; y++) {
		for(uint8_t x = 0; x <= 48-1; x++) {
			Frame[y*7+x] = (y+x)%2;
		}
	}
	// </set frame to 2>
	
	// <declare time>
	uint8_t* Time;
	Time = new uint8_t[6];
	// </declare time>
	
	// <set time>
	Time[0] = 0;
	Time[1] = 1;
	Time[2] = 5;
	Time[3] = 9;
	Time[4] = 5;
	Time[5] = 9;
	// </set time>
	
	TimeToPixels(Time, Frame, Time[5]%2);
	PrintToConsole(Frame);
	
	while(1) {
		Time = DecrTime(Time);	// decrement time
		
		// <print time>
		TimeToPixels(Time, Frame, Time[5]%2);
		PrintToConsole(Frame);
		// </print time>
		
		// check if at end
		if((Time[0]==0)&&(Time[1]==0)&&(Time[2]==0)&&(Time[3]==0)&&(Time[4]==0)&&(Time[5]==0)) {
			break;
		}
	}
}






