int** FrameMerge(int** LastFrame, int** NewFrame) {
	int** Frame;	// <CreateFrameVar>
	Frame = new int*[7];
	for(int i = 0; i<= 6; i++) {
		Frame[i] = new int[48];
	}				// </CreateFrameVar>
	
	for(int FrameYI = 0; FrameYI <= 6; FrameYI++) {
		for(int FrameXI = 0; FrameXI <= 47; FrameXI++) {
			if(NewFrame[FrameYI][FrameXI] == 0) {
				Frame[FrameYI][FrameXI] = 0;
			} else if(NewFrame[FrameYI][FrameXI] == 1) {
				Frame[FrameYI][FrameXI] = 1;
			} else {
				if(LastFrame[FrameYI][FrameXI] == 0) {
					Frame[FrameYI][FrameXI] = 0;
				} else if(LastFrame[FrameYI][FrameXI] == 1) {
					Frame[FrameYI][FrameXI] = 1;
				} else {
					Frame[FrameYI][FrameXI] = 2;
				}
			}
		}
	}
	return(Frame);
}
